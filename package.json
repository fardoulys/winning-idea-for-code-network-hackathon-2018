{
  "name": "thiccify",
  "displayName": "Thiccify",
  "description": "we make your code look more complicated",
  "version": "0.7.1",
  "repository": "https://github.com/samueljim/winning-idea-for-code-network-hackathon-2018",
  "publisher": "thiccify",
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "linter"
  ],
  "activationEvents": [
    "onCommand:extension.thiccify",
    "onCommand:extension.thiccifyDir"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.thiccify",
        "title": "Thiccify"
      },
      {
        "command": "extension.thiccifyDir",
        "title": "Thiccify this folder"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm install && npm run compile && npm prune --production",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "node server.js",
    "now-start": "node server.js",
    "dev": "nodemon -e js,html,css,pug server.js",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "bad-code": "0.0.1",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "remove-blank-lines": "^1.1.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "source-map": "^0.7.3",
    "recast": "0.15.3",
    "util": "^0.11.0",
    "estree-walker": "^0.5.2",
    "clone": "^2.1.2",
    "pug": "^2.0.3"
  }
}
